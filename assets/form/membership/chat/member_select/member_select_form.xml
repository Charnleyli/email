<?xml version="1.0" encoding="utf-8"?>
<root  width="500" height="500" resize="0" padding="1,1,1,1" background="root_0">
<ui class="vlinearlayout" width="fill_parent" height="fill_parent" padding="0,0,0,0" margin="0,0,0,0">
	<ui class="framelayout" id="captionbar" width="fill_parent" height="30">
		<caption class="captionbar" id="caption" width="fill_parent" height="fill_parent" resize="0"/>
		<ui class="linearlayout" width="wrap_content" height="fill_parent" layout_gravity="center|vcenter" margin="0,0,0,0" isview="0" >
			<imageview class="imageview" width="16" height="16" src="app_caption_icon_small" layout_gravity="center|vcenter" padding="0,0,0,0" margin="0,0,5,0" isview="0"/>
			<textview class="textview" id="caption_tex" width="wrap_content" height="fill_parent" text="" font="14n" color="gary1" gravity="center|vcenter" padding="0,0,0,0" isview="0"/>
		</ui>
		<ui class="linearlayout" width="30" height="30" margin="0,0,0,0" layout_gravity="right|top" >
			<sysbutton class="sysbutton" id="sys_close" width="30" height="30" background="button_top_close" icon="sys_close" layout_gravity="right|top" padding="5,5,5,5" margin="0,0,0,0"/>
		</ui>
	</ui>
	<imageview class="imageview" width="fill_parent" height="1" src="line_1" layout_gravity="bottom" margin="0,0,0,0" isview="0"/>
	<ui class="linearlayout" width="fill_parent" height="0" weight="1" padding="0,0,0,0" margin="0,0,0,0">
		<!-- left -->
		<ui class="vlinearlayout"  width="0" height="fill_parent" weight="1"  background="root_1" padding="0,0,0,0">
            <!-- search_frame -->
			<ui class="framelayout" width="fill_parent" height="50" padding="10,0,10,0" background="root_2" visible="0">
				<ui class="framelayout" width="fill_parent" height="29" layout_gravity="center|vcenter" background="search_frame" padding="0,0,0,0" margin="0,0,0,0">
					<edit class="edit" id="filter_edit" width="fill_parent" height="fill_parent" hint="search address keywords" hintfont="12n" hintcolor="gray4" limit="40" padding="10,4,27,4" background="na"/>
					<ui class="framelayout" width="20" height="18" layout_gravity="right|vcenter" padding="0,0,0,0" margin="0,0,5,0" >
						<imageview class="imageview" width="1" height="fill_parent" src="line_3" layout_gravity="left|vcenter" padding="0,2,0,2" isview="0"/>
						<imageview class="imageview" id="search_icon_img" width="16" height="16" src="search_icon" layout_gravity="right|vcenter" margin="0,0,2,0" isview="0" visible="1"/>
						<button class="button" id="search_close_btn" width="16" height="16" icon="search_btn_close" background="na" layout_gravity="right|vcenter" padding="0,0,0,0" margin="0,0,2,0" visible="0"/>
					</ui>
				</ui>
			</ui>
			<ui class="framelayout" height="fill_parent" width="fill_parent" padding="0,0,0,0" >
				<tableview class="tableview" id="left" height="fill_parent" width="fill_parent" padding="0,0,0,0" margin="0,0,0,0" itembg="hlinearlayout"/>
			</ui>
		</ui>
		<!-- right -->
		<ui class="vlinearlayout"  width="0" height="fill_parent" weight="1" background="" padding="0,0,0,0">
            <!-- <ui class="view" width="fill_parent" height="0" isview="0"/> -->
            <textview id="input_tip" class="textview" width="fill_parent" height="50" text="Selected address" font="12n" color="gray3" layout_gravity="left|vcenter" gravity="left|vcenter" margin="0,0,0,0" padding="20,0,0,0" isview="0"/>
            <imageview class="imageview" width="fill_parent" height="1" src="line_3" layout_gravity="left|vcenter" padding="0,0,0,0" isview="0"/>
			<ui class="framelayout" height="0" weight="1" width="fill_parent" padding="0,0,0,0" >
				<tableview class="tableview" id="right" height="fill_parent" width="fill_parent" padding="0,0,0,0" margin="0,0,0,0" itembg="hlinearlayout"/>
			</ui>
			<ui class="linearlayout" width="fill_parent" height="26" margin="0,5,5,5">
				<ui class="view"  width="0" height="0" weight="1"/>
				<button class="button" id="btn_ok" width="90" height="26" background="com_btn" layout_gravity="right|vcenter" text="Ok" color="white" font="12n" margin="0,0,0,0"/>
				<!-- <button class="button" id="close" width="90" height="27" background="com_btn" layout_gravity="right|vcenter" text="Cancel" color="white" font="12n" visible="0"/> -->
			</ui>
		</ui>
	</ui>
</ui>
</root>