<?xml version="1.0" encoding="utf-8"?>
<root id="user_info" width="288" height="200" left="center" top="center" mode="caption" resize="0" >
	<ui class="framelayout" width="fill_parent" height="fill_parent" background="root_0">
		<ui class="framelayout" id="captionbar" width="0" height="0"/>
		<textview class="textview" id="caption_tex" width="0" height="0" text="User info" font="14n" color="black" gravity="center|vcenter" padding="0,0,0,0" isview="0"/>
		<caption class="captionbar" id="caption" width="fill_parent" height="fill_parent" resize="0"/>
		<ui class="vlinearlayout" width="fill_parent" height="fill_parent" margin="0,0,0,0" padding="0,0,0,0" mouse_action="1">
			<!-- avatar -->
			<ui class="linearlayout" width="fill_parent" height="80" margin="0,4,0,0" mouse_action="1">
				<ui class="framelayout" width="56" height="52" layout_gravity="left|vcenter" margin="10,0,0,0" isview="0">
					<imageview class="imageview" id="avatar" width="48" height="48" src="avatar_default" mask="mask_circle_48" gravity="center|vcenter" isview="0"/>
					<checkbox class="checkbox" id="online" width="16" height="16" box="16,16" icon="online" layout_gravity="right|bottom" margin="0,0,10,0" isview="0" />
				</ui>
				<ui class="vlinearlayout" width="fill_parent" height="wrap_content" layout_gravity="left|vcenter" margin="5,0,0,0" >
					<ui class="linearlayout" width="fill_parent" height="wrap_content" layout_gravity="left|vcenter" margin="0,0,32,0">
						<ui class="framelayout" width="wrap_content" height="wrap_content" layout_gravity="left|vcenter" max_limit="110,-1">
							<edit class="edit" id="name" width="fill_parent" height="wrap_content" color="gray" font="16n" layout_gravity="left|vcenter" margin="0,0,0,0" padding="0,0,0,0" background="na"  readonly="1" ellipsis_end="1"/>
						</ui>
						<ui class="view" width="0" height="fill_parent" weight="1" isview="0" />
						<textview class="textview" id="tourist_label" width="wrap_content" height="20" text="Tourist" color="white" font="12n" background="root_a_128"  layout_gravity="center|bottom" gravity="center|vcenter"  padding="4,0,4,0" margin="5,0,0,0" isview="0" visible="0"/>
						<textview class="textview" id="offline_label" width="wrap_content" height="20" text="Offline" color="white" font="12n" background="root_a_128"  layout_gravity="center|bottom" gravity="center|vcenter"  padding="4,0,4,0" margin="5,0,0,0" isview="0" visible="0"/>
						<textview class="textview" id="online_label" width="wrap_content" height="20" text="Online" color="white" font="12n" background="root_green"  layout_gravity="center|bottom" gravity="center|vcenter"  padding="4,0,4,0" margin="5,0,0,0" isview="0" visible="0"/>
						<button class="button" id="memo_lab" width="wrap_content" height="20" layout_gravity="center|bottom" gravity="center|vcenter" background="com_btn" text="memo" color="white" font="12n" margin="8,0,0,0" padding="6,0,6,0" visible="0"/>
					</ui>
					<!-- MM ID -->
					<ui class="linearlayout" width="fill_parent" height="wrap_content" margin="0,8,0,0">
						<textview class="textview" id="mm_id" width="wrap_content" height="20" text="YoMail ID" color="gray3" font="12n" padding="0,0,0,0" gravity="left|vcenter" isview="0"/>
						<textview class="textview"  width="wrap_content" height="20" text=":" color="gray3" font="12n" margin="3,0,0,0" gravity="left|vcenter" isview="0"/>
						<edit class="edit" id="user_id" width="fill_parent" height="20" text="" color="gray3" font="12n" padding="0,0,0,0" margin="10,0,0,0" gravity="left|vcenter" readonly="1" background="na"/>
					</ui>
				</ui>
			</ui>
			<ui class="vlinearlayout" width="fill_parent" height="0" weight="1" >
				<!-- EMail -->
				<ui class="linearlayout" width="fill_parent" height="20" margin="10,0,0,0" mouse_action="1">
					<textview class="textview" id="email_lab" width="56" height="fill_parent" text="EMail" color="gray3" font="12n" padding="10,0,10,0" gravity="left|vcenter" isview="0"/>
					<edit class="edit" id="email" width="fill_parent" height="fill_parent" text="" color="gray3" font="12n" padding="0,0,0,0" margin="6,0,0,0" gravity="left|vcenter" background="na" readonly="1"/>
				</ui>
				<!-- group nick name -->
				<ui class="linearlayout" id="group_nickname_panel" width="fill_parent" height="26" margin="10,4,0,0">
					<textview class="textview" id="" width="56" height="fill_parent" text="Group NickName" color="gray3" font="12n" padding="10,0,10,0" gravity="left|vcenter" isview="0"/>
					<ui class="linearlayout" id="inner_edit_panel" width="0" weight="1" height="26" layout_gravity="left|vcenter" margin="6,0,0,0" mouse_action="1" visible="0" >
						<edit class="edit" id="inner_info_edit" width="0" weight="1" height="20" text="" color="gray" font="12n" padding="5,0,0,0" margin="0,0,0,0" layout_gravity="left|vcenter" gravity="left|vcenter" readonly="0" background=""/>
						<button class="button" id="inner_btn_save" cursor="hand" width="30" height="24" box="26,26" text="Save" color="white" font="12n" layout_gravity="center|vcenter" gravity="center|vcenter" margin="5,0,5,0" background="com_btn"  />
						<button class="button" id="inner_btn_cancel" cursor="hand" width="wrap_content" height="24" box="26,26" text="Cancel" color="white" font="12n" layout_gravity="center|vcenter" gravity="center|vcenter" margin="5,0,5,0" background="com_btn"  />
					</ui>
					<ui class="linearlayout" id="inner_display_panel" width="0" weight="1" height="26" layout_gravity="left|vcenter" margin="6,0,0,0" mouse_action="1" >
						<edit class="edit" id="inner_info_display" width="wrap_content" height="20" text="" color="gray" font="12n"  margin="0,0,8,0" layout_gravity="left|vcenter" gravity="left|vcenter" readonly="1" background="na"/>
						<button class="button" id="inner_btn_edit" cursor="hand" width="26" height="26" box="26,26" text="Edit" font="12n_underline" color="blue1_status" layout_gravity="left|vcenter" gravity="left|vcenter" margin="0,0,20,0" background="na"  />
					</ui>
				</ui>
				<!-- Memo -->
				<ui class="linearlayout" id="memo_panel" width="fill_parent" height="26" margin="10,3,0,0">
					<textview class="textview" id="" width="56" height="fill_parent" text="Memo" color="gray3" font="12n" padding="10,0,10,0" gravity="left|vcenter" isview="0"/>
					<ui class="linearlayout" id="inner_edit_panel" width="0" weight="1" height="26" layout_gravity="left|vcenter" margin="6,0,0,0" mouse_action="1" visible="0" >
						<edit class="edit" id="inner_info_edit" width="0" weight="1" height="20" text="" color="gray" font="12n" padding="5,0,0,0" margin="0,0,0,0" layout_gravity="left|vcenter" gravity="left|vcenter" readonly="0" background=""/>
						<button class="button" id="inner_btn_save" cursor="hand" width="30" height="24" box="26,26" text="Save" color="white" font="12n" layout_gravity="center|vcenter" gravity="center|vcenter" margin="10,0,20,0" background="com_btn"  />
						<button class="button" id="inner_btn_cancel" cursor="hand" width="wrap_content" height="24" box="26,26" text="Cancel" color="white" font="12n" layout_gravity="center|vcenter" gravity="center|vcenter" margin="10,0,20,0" background="com_btn"  />
					</ui>
					<ui class="linearlayout" id="inner_display_panel" width="0" weight="1" height="26" layout_gravity="left|vcenter" margin="6,0,0,0" mouse_action="1" >
						<edit class="edit" id="inner_info_display" width="wrap_content" height="20" text="" color="gray" font="12n"  margin="0,0,8,0" layout_gravity="left|vcenter" gravity="left|vcenter" readonly="1" background="na"/>
						<button class="button" id="inner_btn_edit" cursor="hand" width="26" height="26" box="26,26" text="Edit" font="12n_underline" color="blue1_status" layout_gravity="left|vcenter" gravity="left|vcenter" margin="0,0,20,0" background="na"  />
					</ui>
				</ui>
			</ui>
			<imageview class="imageview" width="fill_parent" height="1" src="line_2" margin="24,0,24,0" gravity="center|vcenter" isview="0"/>
			<ui class="linearlayout" width="fill_parent" height="34" margin="24,0,0,0" mouse_action="1">
				<button class="button" id="reply_chat" width="24" height="20" icon="info_card_chat" tooltip="Chat" layout_gravity="center|vcenter" color="gray1" font="12n" background="tool_btn_hover" padding="2,0,2,0" margin="0,0,20,0"/>
				<button class="button" id="reply_email" width="24" height="20" icon="info_card_email" tooltip="SendMail" layout_gravity="center|vcenter" color="gray1" font="12n" background="tool_btn_hover" padding="2,0,2,0" margin="0,0,0,0"/>
				<ui class="view" width="0" weight="1" height="fill_parent" isview="0"/>
				<ui class="framelayout" id="add_buddy" width="wrap_content" height="20" layout_gravity="left|vcenter" margin="15,0,25,0" background="tool_btn">
					tool_btn_hover
					<imageview class="imageview" width="16" height="16" src="popup_email_add_black" gravity="center|vcenter" layout_gravity="left|vcenter" margin="10,0,0,0" isview="0"/>
					<textview class="textview" width="fill_parent" height="wrap_content" text="btn_add_friend" font="13n" color="gray3" gravity="left|vcenter" layout_gravity="left|vcenter" margin="30,0,10,0" isview="0"/>
				</ui>
			</ui>
			<!-- toolbar end-->
		</ui>
		<ui class="linearlayout" width="30" height="30" margin="0,0,0,0" layout_gravity="right|top" >
			<sysbutton class="sysbutton" id="sys_close" width="30" height="30" background="button_top_close" icon="sys_close" layout_gravity="right|top" padding="5,5,5,5" margin="0,0,0,0"/>
		</ui>
		<ui class="vlinearlayout" width="0" height="0" padding="0,0,0,0" margin="0,0,0,0" background="root_2" visible="0">
			<!-- Signature -->
			<ui class="linearlayout" width="fill_parent" height="30" margin="10,8,0,8">
				<textview class="textview" id="signature_lab" width="100" height="fill_parent" padding="10,0,10,0" text="Signature" singleline="1" color="gray3" font="13n" gravity="left|vcenter"/>
				<textview class="textview" width="5" height="fill_parent" padding="0,0,0,0" margin="0,0,0,0" text=":" singleline="1" color="gray3" font="14n" gravity="vcenter"/>
				<edit class="edit" readonly="1" id="signature" width="fill_parent" height="fill_parent" text="4" singleline="1" color="gray1" font="12n" padding="10,5,2,5" limit="50" margin="10,0,15,0" background="na" hint="[ User haven't status message ]" hintfont="14n" hintcolor="gray4"/>
			</ui>
		</ui>
	</ui>
</root>