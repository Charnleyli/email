<?xml version="1.0" encoding="utf-8"?>
<root id="user_info" width="680" height="500" left="center" top="center" mode="caption" resize="0" padding="0,0,0,0" background="root_0">
<ui class="framelayout" width="fill_parent" height="fill_parent">
	<ui class="linearlayout" width="fill_parent" height="fill_parent" margin="0,30,0,0" >
		<ui class="vlinearlayout" width="0" weight="1" height="fill_parent" >showrect="1"
			<imageview class="imageview" id="m_avatar_big_imageView" width="100" height="100" layout_gravity="center|vcenter" margin="0,50,0,0"/>
			<textview class="textview" id="m_left_nickname_lab" width="wrap_content" height="wrap_content"  layout_gravity="center|vcenter" text="我最爱的米老鼠" gravity="center|vcenter" font="14B" color="black" />
			<ui class="linearlayout" width="fill_parent" height="wrap_content" margin="0,5,0,0" >showrect="1"
				<textview class="textview" width="wrap_content" height="wrap_content"  font="12n" text="登陆邮箱:" margin="10,5,0,0"/>
				<textview class="textview" id="m_left_email_lab" width="wrap_content" height="wrap_content"  font="12n" text="xx@qq.com" margin="0,5,0,0"/>
			</ui>
			<ui class="linearlayout" width="fill_parent" height="wrap_content" margin="0,5,0,0" >showrect="1"
				<textview class="textview" width="wrap_content" height="wrap_content" font="12n" text="密码:" margin="10,10,0,0"/>
				<textview class="textview" id="m_passwd_lab" width="wrap_content" height="wrap_content" font="12n" text="**********" margin="0,10,0,0"/>
			</ui>
			<button class="button" id="m_personCenter_btn" width="fill_parent" height="40" margin="10,40,10,0" font="14n" text="网页个人中心" background="com_btn_white_box_r2" color="blue1"/>
		</ui>

		<ui class="vlinearlayout" width="0" weight="2" height="fill_parent" >showrect="1"
			<ui class="vlinearlayout" width="fill_parent" weight="1" height="0" >showrect="1"
				<ui class="linearlayout" width="fill_parent" weight="1" height="0" >showrect="1"
					<textview class="textview" width="fill_parent" height="fill_parent" layout_gravity="vcenter" margin="10,10,0,0" gravity="left|vcenter" text="YoMail头像" font="14B"/>
				</ui>
				<ui class="linearlayout" width="fill_parent" weight="1" height="0" >showrect="1"
					<imageview class="imageview" id="m_avatar_small_imageView" width="32" height="32" margin="10,10,0,0" mask="mask_circle_32"/>
					<button class="button" id="m_avatar_change_btn" width="80" height="32" margin="10,10,0,0" font="14n" text="更换头像" background="com_btn_white_box_r2" color="blue1"/>
					<button class="button" id="m_avatar_remove_btn" width="80" height="32" margin="10,10,0,0" font="14n" text="移除头像" background="com_btn_white_box_r2" color="blue1"/>
				</ui>
			</ui>

			<ui class="vlinearlayout" width="fill_parent" weight="1" height="0" >showrect="1"
				<ui class="linearlayout" width="fill_parent" height="0" weight="1" >showrect="1"
					<textview class="textview" width="100" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="YoMailID" font="14B"/>
					<textview class="textview" id="m_id_lab" width="100" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="123456"/>
				</ui>
				<ui class="linearlayout" width="fill_parent" height="0" weight="1" >showrect="1"
					<textview class="textview" width="100" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="姓名" font="14B"/>
					<textview class="textview" id="m_right_nickname_display" width="wrap_content" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="我最爱的老鼠"/>
					<button class="button" id="m_edit_nickname_btn" width="wrap_content" height="wrap_content" margin="10,10,0,0" font="14n" text="编辑" background="com_btn_white_box_r2" color="blue1"/>
					<edit class="edit" id="m_edit_nickname" width="wrap_content" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="我最爱的老鼠"/>
					<button class="button" id="m_save_nickname_btn" width="wrap_content" height="wrap_content" margin="10,10,0,0" font="14n" text="保存" background="com_btn_white_box_r2" color="blue1"/>
				</ui>
				<ui class="linearlayout" width="fill_parent" height="0" weight="1" >showrect="1"
					<textview class="textview" width="100" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="当前等级" font="14B"/>
					<textview class="textview" id="m_level_lab" width="wrap_content" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="Lv.37"/>

					<button class="button" id="m_member_btn" width="wrap_content" height="wrap_content" text="成为会员" layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" visible="0" background="com_btn_white_box_r2" color="blue1"/>

					<textview class="textview" id="m_vip_lab" width="wrap_content" height="wrap_content" text="vip" layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0"/>
					<textview class="textview" id="m_time_lab" width="wrap_content" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="2017年10月"/>
					<button class="button" id="m_renew_btn" width="30" height="20" margin="10,10,0,0" font="14n" color="blue" text="续费" background="com_btn_white_box_r2" color="blue1"/>
				</ui>
			</ui>

			<ui class="vlinearlayout" width="fill_parent" weight="1" height="0" >showrect="1"
				<ui class="linearlayout" width="fill_parent" height="0" weight="1" >showrect="1"
					<textview class="textview" width="100" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="登陆邮箱" font="14B"/>
					<textview class="textview" id="m_right_email_lab" width="wrap_content" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="xxxx@qq.com"/>
					<button class="button" id="m_change_emil_btn" width="wrap_content" height="20" margin="10,15,0,0" font="14n" text="修改登陆邮箱" background="com_btn_white_box_r2" color="blue1"/>
				</ui>
				<ui class="linearlayout" width="fill_parent" height="0" weight="1" >showrect="1"
					<textview class="textview"  width="100" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="手机号" font="14B"/>
					<textview class="textview" id="m_iphone_lab" width="wrap_content" height="wrap_content"  layout_gravity="vcenter" gravity="left|vcenter" margin="10,0,0,0" text="11111111111"/>
				</ui>
	
			</ui>

			<ui class="framelayout" width="fill_parent" weight="1" height="0" >showrect="1"
					<button class="button" id="m_logout_btn" width="80" height="32"  layout_gravity="vcenter|center" margin="10,50,0,0" text="注销账号" font="14B" background="com_btn_white_box_r2"/>
			</ui>
		</ui>
	</ui>
	<!--right button  -->
	<ui class="framelayout" width="fill_parent" height="30" >showrect="1"
		<caption class="captionbar" id="caption" width="fill_parent" height="fill_parent" resize="0"/>
		<ui class="linearlayout" width="30" height="30" margin="0,0,0,0" layout_gravity="right|top" >
			<!-- system -->
			<sysbutton class="sysbutton" id="sys_close" width="30" height="30" background="button_top_close" icon="sys_close" layout_gravity="right|top" padding="5,5,5,5"/>
		</ui>
	</ui>
</ui>
</root>